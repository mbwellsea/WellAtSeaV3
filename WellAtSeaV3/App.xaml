<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="WellAtSeaV3.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WellAtSeaV3">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <!--  WellAtSea Brand Colors  -->
            <Color x:Key="BrandColor">#0076bf</Color>
            <Color x:Key="TextColor">#484848</Color>
            <Color x:Key="TextTitleColor">#CCCACA</Color>
            <Color x:Key="BackgroundColor">#F9F9F5</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>
